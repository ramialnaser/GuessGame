@model AdminViewModel
@{
	ViewData["Title"] = "Admin";
}

@Html.Raw(PrintModel.Title("Admin Page", 1))
@Html.Raw(PrintModel.Title("Number of users" + Model.Users.Count, 1))


@foreach (var item in Model.Users)
{
	@Html.Raw(PrintModel.UserForm(item))
	<br />
}


@Html.Raw(PrintModel.Title("Number of games" + Model.Games.Count + " ", 1))
@foreach (var item in Model.Games)
{
	@Html.Raw(PrintModel.GameForm(item))
	<br />
}



